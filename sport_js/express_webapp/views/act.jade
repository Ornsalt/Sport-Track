doctype html

html(lang='en')
    head
        link(rel='stylesheet', href='/stylesheets/style.css')
        link(rel='stylesheet', href='/stylesheets/Yorha.css')
        title=name

    header
        h1=name

    body
        table(border='1')
            thead
                tr
                    th Description
                    th Date
                    th Start
                    th Duration
                    th Min
                    th Avg
                    th Max
                    th Duration

            tbody
                each val in data
                    tr
                        td=val.description
                        td=val.date
                        td=val.start
                        td=val.duration
                        td=val.cardMin
                        td=val.cardAvg
                        td=val.cardMax
                        td=val.duration